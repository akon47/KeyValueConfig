    <PackageReleaseNotes>first release</PackageReleaseNotes>
    <PackageProjectUrl>https://github.com/akon47/KeyValueConfig</PackageProjectUrl>
  </PropertyGroup>

  <ItemGroup>
    <None Include="logo.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
